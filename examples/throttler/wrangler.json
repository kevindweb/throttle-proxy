{
  "name": "throttler",
  "main": "src/index.ts",
  "compatibility_date": "2024-12-05",
  "observability": {
    "enabled": true
  },
  "vars": {
    "UPSTREAM": "jsonplaceholder.typicode.com",
    "USE_HTTPS": true,
    "ENABLE_JITTER": true,
    "JITTER_DELAY": 100,
    "BACKPRESSURE": {
      "ENABLED": true,
      "CWDN_MIN": 10,
      "CWDN_MAX": 100,
      "QUERIES": [
        {
          "NAME": "test",
          "QUERY": "test",
          "WARN_THRESHOLD": 4,
          "EMERGENCY_THRESHOLD": 15
        }
      ]
    }
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "QUERY_VALUES",
        "class_name": "QueryValues"
      },
      {
        "name": "COUNTERS",
        "class_name": "Counter"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": [
        "Counter"
      ]
    }
  ],
  "tail_consumers": [
    {
      "service": "throttler"
    }
  ]
}
